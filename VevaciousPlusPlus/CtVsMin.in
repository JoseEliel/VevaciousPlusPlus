set term postscript eps enhanced color solid
set ylabel "field/GeV"
set xlabel "rho/(1/GeV)"
set output "MinuitBubbleProfile.eps"
set style line 1 lt rgb "red" lw 3
set style line 2 lt rgb "purple" lw 3
set style line 3 lt rgb "blue" lw 3
set style line 4 lt rgb "green" lw 3
plot 'gnuplot_data.txt0' index 0 title "Hd" with lines ls 1, 'gnuplot_data.txt1' index 0 title "Hu" with lines ls 2, 'gnuplot_data.txt2' index 0 title "Q[1,3]" with lines ls 3, 'gnuplot_data.txt3' index 0 title "U[3]" with lines ls 4
set output "CtBubbleProfile.eps"
plot 'CtBubbleProfile.txt0' index 0 title "Hd" with lines ls 1, 'CtBubbleProfile.txt1' index 0 title "Hu" with lines ls 2, 'CtBubbleProfile.txt2' index 0 title "Q[1,3]" with lines ls 3, 'CtBubbleProfile.txt3' index 0 title "U[3]" with lines ls 4
set output "CtVsMinuit0.eps"
plot 'CtBubbleProfile.txt0' index 0 title "Ct Hd" with lines ls 1, 'gnuplot_data.txt0' index 0 title "Min Hd" with lines ls 3
set output "CtVsMinuit1.eps"
plot 'CtBubbleProfile.txt1' index 0 title "Ct Hu" with lines ls 1, 'gnuplot_data.txt1' index 0 title "Min Hu" with lines ls 3
set output "CtVsMinuit2.eps"
plot 'CtBubbleProfile.txt2' index 0 title "Ct Q[1,3]" with lines ls 1, 'gnuplot_data.txt2' index 0 title "Min Q[1,3]" with lines ls 3
set output "CtVsMinuit3.eps"
plot 'CtBubbleProfile.txt3' index 0 title "Ct U[3]" with lines ls 1, 'gnuplot_data.txt3' index 0 title "Min U[3]" with lines ls 3
